    "gsap": "^3.6.0",
    "nuxt": "^2.15.1",
    "nuxt-i18n": "^6.20.0",
    "reading-time": "^1.3.0",
    "remark-emoji": "^2.1.0",
    "vue-click-outside": "^1.1.0",
    "vue-flag-icon": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
